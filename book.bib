@article{10.1001/jamanetworkopen.2019.18040,
    author = {Centor, Robert M. and Geha, Rabih and Manesh, Reza},
    title = "{The Pursuit of Diagnostic Excellence}",
    journal = {JAMA Network Open},
    volume = {2},
    number = {12},
    pages = {e1918040-e1918040},
    year = {2019},
    month = {12},
    abstract = "{A 1957 study of diagnostic errors starts with this sentence: “The cornerstone of internal medicine is correctness of diagnosis.”(p108) This concept readily extends to most physician specialties. Without the correct diagnosis, our advice to patients will lack accuracy about treatment possibilities and prognosis. Because correct diagnosis has such importance, medical education should stress diagnostic excellence as a key goal.Brush and colleagues discuss one aspect of diagnostic excellence: probability revision. Medical students revise probabilities at a better rate than chance alone. Brush and colleagues found that students who received specific training on posttest probability revision performed slightly better on a paper task than those who learned about some disease manifestations, but all moved toward the correct posttest probability.}",
    issn = {2574-3805},
